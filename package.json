  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "laravel-mix": "^6.0.49",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.7"
  },
  "dependencies": {
    "@alpinejs/persist": "^3.10.3",
    "alpinejs": "^3.10.3",
